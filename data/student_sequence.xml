<odoo>
    <record id="seq_student_code" model="ir.sequence">
        <field name="name">Student Code</field>
        <field name="code">student.master.code</field>
        <field name="prefix">STU</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_subject_code" model="ir.sequence">
        <field name="name">Subject Code</field>
        <field name="code">subject.master.code</field>
        <field name="prefix">SUB</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>

